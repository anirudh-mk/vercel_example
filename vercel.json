{
  "builds": [{
    "src": "vercel_app/wsgi.py",
    "use": "@ardnt/vercel-python-wsgi",
    "config": {
      "maxLambdaSize": "15mb",
      "runtime": "python3.9",
      "outputDirectory": "api"  // Specify output directory if needed
    }
  }],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "vercel_app/wsgi.py"
    }
  ]
}